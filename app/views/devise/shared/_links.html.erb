<%- if controller_name != 'sessions' %>
  <%= link_to "会員の方はこちら", new_session_path(resource_name), class: "btn signin_button" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "会員登録がまだの方", new_registration_path(resource_name), class: "btn signin_button" %><br />
<% end %>


