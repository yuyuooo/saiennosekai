<%- if controller_name != 'sessions' %>
  <%= link_to "会員の方はこちらからログイン", new_session_path(resource_name), class: "btn btn-info" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "会員登録がお済みでない方はこちら", new_registration_path(resource_name), class: "btn btn-info" %><br />
<% end %>


