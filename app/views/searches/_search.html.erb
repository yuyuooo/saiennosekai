<% if user_signed_in? %>
  <%= form_with url: search_path, local: true, method: :get do |f| %>
    <%= f.text_field :content %>
    <%= f.select :model, options_for_select([['ユーザー','User'],['作物','CropFolder'],['商品','Item']]) %>
    <%= f.submit "検索", class: "btn btn-warning btn-sm" %>
  <% end %>
<% end %>